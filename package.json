    "build:react": "react-scripts build",
    "build:css": "NODE_ENV=production postcss src/styles/index.css -o src/index.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
